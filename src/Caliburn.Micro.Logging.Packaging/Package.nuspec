<?xml version="1.0"?>
<package >
  <metadata>
    <id>Caliburn.Micro.Logging</id>
    <version>2.0.1-beta2</version>
    <title>Caliburn.Micro.Logging</title>
    <authors>David Buksbaum</authors>
    <owners>David Buksbaum</owners>
    <projectUrl>http://buksbaum.us/2011/07/04/introducing-caliburn-micro-logging/</projectUrl>
    <iconUrl>http://www.eyecatch.no/favicon.ico</iconUrl>
    <licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Provides core logging functionality for Caliburn.Micro
    </description>
    <summary>
      Core logging implementation for Caliburn.Micro, along with a derived extension to the ILog interface to support richer Error messages.
    </summary>
    <releaseNotes>
      * Upgraded solution / project files to Visual Studio 2013
      * Upgraded Caliburn.Micro 1.4 to Caliburn.Micro.Core 2.0.0-beta2
      * Converted Caliburn.Micro.Logging to a portable class library
      * Dropped support for TraceLogger since it is not support in a PCL
      * Dropped SilverLight 4/5 and Windows Phone 7.1 support
      * Added a key for strong name support
    </releaseNotes>
    <copyright>Copyright David Buksbaum 2009-2014</copyright>
    <tags>Caliburn.Micro Logging</tags>
    <dependencies>
      <dependency id="Caliburn.Micro.Core" version="2.0.0-beta2" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\Caliburn.Micro.Logging\bin\Release\Caliburn.Micro.Logging.*" target="lib\portable-net45+win8+wp8+wpa81" />
    <!--file src="tools\**" target="tools" />
    <file src="content\**" target="content" /-->
    <file src="..\Caliburn.Micro.Logging\DebugLogger.cs" target="content\Caliburn\Micro\Logging\DebugLogger.cs" />
  </files>
</package>